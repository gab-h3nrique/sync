{
  "name": "sync",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm:server\" \"npm:app\"",
    "server": "nodemon --env-file=.env src/server.ts",
    "app": "vite --host",
    "build": "rm -rf ./dist/ && npx tsc --project tsconfig.json && vite build",
    "react:build": "vite build",
    "start": "node --env-file=.env ./dist/server.js",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "bcrypt": "^5.1.1",
    "concurrently": "^9.1.2",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.8",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.1.4",
    "postcss": "^8.5.4",
    "prisma": "^6.8.2",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.5.4",
    "vite": "^6.3.5"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
